    "submit_errors_list" : {
      "type" : "expression",
      "value" : "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){\n\tconst liElements = $data.formOutput._submitError.explanations\n\t\t.filter(cause => cause !== null)\n\t\t.map(cause => \"<li>\" + cause + \"</li>\")\n\t\t.join('');\n\tif(liElements){\n\t\treturn \"<ul>\" + liElements + \"</ul>\";\n\t}\n}",
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : "var isKm = $data.isKm;\nreturn {\n    \"persistenceId\": $data.expenseId,\n    \"description\": $data.expense.description,\n    \"expenseDate\": $data.expense.expenseDate,\n    \"currency\": isKm? \"\" : $data.expense.currency,\n    \"isKm\": isKm,\n    \"expenseType\": $data.expense.expenseType,\n    \"amount\": isKm ? $data.kmAmount : $data.expense.amount,\n    \"km\": isKm ? $data.expense.km : 0,\n    \"vehicleType\": isKm ? $data.expense.vehicleType : \"\"\n}",
      "exposed" : false
    },
    "context" : {
      "type" : "url",
      "value" : "../API/bpm/userTask/{{taskId}}/context",
      "exposed" : false
    },
