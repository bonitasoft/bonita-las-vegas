                        <queryParameter name="endDate" className="java.util.Date"/>
                        <queryParameter name="status" className="java.lang.String"/>
                    </queryParameters>
                </query>
                <query name="findPendingAndApprovedAfter" content="SELECT v &#xA;FROM VacationRequest v &#xA;WHERE v.startDate &gt;= :startDate&#xA;AND v.status IN ('pending','approved')&#xA;ORDER BY v.startDate,v.persistenceId DESC" returnType="java.util.List">
